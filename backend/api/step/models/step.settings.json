{
  "kind": "collectionType",
  "collectionName": "steps",
  "info": {
    "name": "Step",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
